{
  "name": "leopold",
  "version": "0.1.7",
  "description": "Event-sourced state support",
  "main": "./dist/leopold.js",
  "scripts": {
    "clean": "rimraf dist/* && mkdir dist || true",
    "test": "babel-tape-runner ./test/**/*-spec.js | faucet",
    "build": "npm run clean && babel src --out-dir dist",
    "docs": "doxx --source src --target docs",
    "gh-pages": "npm run docs && gh-pages -d docs",
    "prepublish": "npm run build && npm test && npm run gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mnichols/leopold"
  },
  "keywords": [
    "event-sourcing",
    "esp",
    "meerkats"
  ],
  "author": "Mike Nichols <nichols.mike.s@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mnichols/leopold/issues"
  },
  "homepage": "https://github.com/mnichols/leopold",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "blacklist": "regenerator"
        }
      ]
    ]
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-tape-runner": "^1.2.0",
    "babelify": "^6.1.3",
    "blue-tape": "^0.1.10",
    "browser-run": "^2.1.2",
    "browserify": "^11.0.1",
    "doxx": "^2.0.0",
    "faucet": "0.0.1",
    "gh-pages": "^0.4.0",
    "rimraf": "^2.4.3"
  },
  "dependencies": {
    "bluebird": "^2.9.34",
    "cuid": "^1.2.5",
    "stampit": "^2.1.0"
  }
}
